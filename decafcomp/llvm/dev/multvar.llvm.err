; ModuleID = 'Test'

declare void @print_int(i32)

define i32 @main() {
entry:
  %x = alloca i32
  store i32 0, i32* %x
  %y = alloca i32
  store i32 0, i32* %y
  store i32 1, i32* %x
  store i32 1, i32* %y
  %ld_x = load i32, i32* %x
  call void @print_int(i32 %ld_x)
  %ld_y = load i32, i32* %y
  call void @print_int(i32 %ld_y)
  ret i32 0
}
